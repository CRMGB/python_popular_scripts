   "source": [
    "# Execution time: 0.0000119209289550781250\n",
    "@timing\n",
    "def fibonacci(N):\n",
    "    result = []\n",
    "    a, b = 1, 2\n",
    "    for _ in range(N):\n",
    "        result.append(a)\n",
    "        a, b = b, a + b\n",
    "    return result\n",
    "\n",
    "print(fibonacci(10))"
   ]
